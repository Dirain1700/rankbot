{
    "extends": ["eslint:recommended"],
    "plugins": [],
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "globals": {
      "config": true,
      "fs": false
    },
    "rules": {
      "no-unused-vars": 1,
      "semi": [1,"always"],
      "no-extra-semi": 1
    },
    "parserOptions": {
        "ecmaVersion": 2021
    }
}
