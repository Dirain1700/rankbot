{
    "extends": ["eslint:recommended"],
    "plugins": [],
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "globals": {
      "config": true,
      "fs": false,
      "path": false,
      "time": false,
      "tool": false
    },
    "rules": {
      "no-unused-vars": "error",
      "semi": ["warn","always"],
      "no-extra-semi": "warn",
      "no-var": "error"
    },
    "parserOptions": {
        "ecmaVersion": 2021
    }
}
