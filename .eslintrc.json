{
    "extends": ["eslint:recommended"],
    "plugins": [],
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "globals": {
      "config": true,
      "fs": false,
      "path": false,
      "time": false,
      "tool": false
    },
    "rules": {
      "no-unused-vars": 1,
      "semi": [1,"always"],
      "no-extra-semi": 1,
      "no-var": 2
    },
    "parserOptions": {
        "ecmaVersion": 2021
    }
}
