{
    "name": "mogi-bot",
    "version": "1.0.0",
    "description": "No bot, No life.",
    "main": "index.js",
    "scripts": {
        "start": "npm run build && node --enable-source-maps --trace-warnings dist/src/index.js",
        "pre-tsc": "tsc -p tsconfig.json --noEmit",
        "tsc": "tsc -p tsconfig.json",
        "build": "npm run write && npm run lint-fix && npm run tsc",
        "ci": "npm run check && npm run lint && npm run pre-tsc",
        "lint": "eslint \"src/**/*.ts\"",
        "lint-fix": "eslint --fix \"src/**/*.ts\"",
        "write": "prettier --write .",
        "check": "prettier --check ."
    },
    "dependencies": {
        "@dirain/client": "^0.0.20",
        "@types/lodash": "^4.14.191",
        "axios": "^1.3.4",
        "discord.js": "^14.7.1",
        "lodash": "^4.17.21",
        "node-schedule": "^2.1.1"
    },
    "devDependencies": {
        "@types/node": "^18.14.1",
        "@types/node-schedule": "^2.1.0",
        "@types/prettier": "^2.7.2",
        "@typescript-eslint/eslint-plugin": "5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.25.0",
        "prettier": "^2.8.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=16.9.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dirain1700/rankbot.git"
    },
    "author": "Dirain1700",
    "bugs": {
        "url": "https://github.com/Dirain1700/rankbot/issues"
    },
    "license": "MIT"
}
