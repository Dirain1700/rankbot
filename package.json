{
    "name": "mogi-bot",
    "version": "0.9.0",
    "description": "No bot, No life.",
    "main": "index.js",
    "scripts": {
        "start": "node dist/src/index.js",
        "pre-tsc": "tsc -p tsconfig.json --noEmit",
        "tsc": "tsc -p tsconfig.json",
        "build": "npm run write && npm run lint-fix && npm run tsc",
        "ci": "npm run check && npm run lint && npm run pre-tsc",
        "lint": "eslint \"src/**/*.ts\"",
        "lint-fix": "eslint --fix \"src/**/*.ts\"",
        "write": "prettier --write .",
        "check": "prettier --check ."
    },
    "dependencies": {
        "@dirain/client": "^0.0.9",
        "discord.js": "^14.7.1",
        "node-schedule": "^2.1.0",
        "vm2": "^3.9.11",
        "workerpool": "^6.3.1"
    },
    "devDependencies": {
        "@types/node": "^18.11.10",
        "@types/node-schedule": "^2.1.0",
        "@types/workerpool": "^6.1.0",
        "@typescript-eslint/eslint-plugin": "5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "prettier": "^2.8.0",
        "typescript": "^4.9.3"
    },
    "engines": {
        "node": ">=16.9.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dirain1700/rankbot.git"
    },
    "author": "Dirain1700",
    "bugs": {
        "url": "https://github.com/Dirain1700/rankbot/issues"
    },
    "license": "MIT"
}
