{
    "name": "mogi-bot",
    "version": "1.0.0",
    "description": "No bot, No life.",
    "main": "index.js",
    "scripts": {
        "start": "npm run esbuild && node --enable-source-maps --trace-warnings dist/src/index.js",
        "tsc": "tsc -p tsconfig.json",
        "esbuild": "node --enable-source-maps build.js",
        "setup-zh": "node --enable-source-maps dist/src/detect-zh/setup/setup.js",
        "pre-build": "tsc -p tsconfig.build.json",
        "build": "npm run write && npm run lint-fix && npm run esbuild",
        "ci": "npm run check && npm run lint && npm run tsc",
        "lint": "eslint --cache .",
        "lint-fix": "eslint --cache --fix .",
        "write": "prettier --write .",
        "check": "prettier --check .",
        "ncu": "npx -p npm-check-updates  -c \"ncu -u\""
    },
    "dependencies": {
        "@dirain/client": "^0.0.26-dev.1691349683-f6a0cdf.0",
        "axios": "^1.4.0",
        "discord.js": "^14.12.1",
        "eslint-plugin-import": "^2.28.0",
        "lodash": "^4.17.21",
        "node-schedule": "^2.1.1"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.196",
        "@types/node": "^20.4.7",
        "@types/node-schedule": "^2.1.0",
        "@types/prettier": "^2.7.3",
        "@typescript-eslint/eslint-plugin": "6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "esbuild": "^0.18.17",
        "eslint": "^8.46.0",
        "glob": "^10.3.3",
        "prettier": "^3.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "engines": {
        "node": ">=16.9.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dirain1700/rankbot.git"
    },
    "author": "Dirain1700",
    "bugs": {
        "url": "https://github.com/Dirain1700/rankbot/issues"
    },
    "license": "MIT"
}
